{
    "provider": {
        "type": "local",
        "os": "debian",
        "local": {
            "url": "http:\/\/puppet-vagrant-boxes.puppetlabs.com\/debian-70rc1-x64-vbox4210.box",
            "name": "wheezy64",
            "personal_name": "my-first-box",
            "ip": "192.168.56.101",
            "memory": "1024",
            "port_forward": {
                "host": "",
                "guest": ""
            },
            "foldertype": "default",
            "synced_folder": {
                "source": ".\/",
                "target": "\/var\/www"
            }
        },
        "digitalocean": {
            "name": "puphpet_digitalocean",
            "size": "512MB",
            "region": "New York 1",
            "client_id": "DIGITAL_OCEAN_CLIENT_ID",
            "api_key": "DIGITAL_OCEAN_API_KEY",
            "private_key": "~\/.ssh\/id_rsa",
            "synced_folder": {
                "source": ".\/",
                "target": "\/var\/www"
            }
